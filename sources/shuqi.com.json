{
    "name": "书旗小说",
    "url": "shuqi.com",
    "version": 100,
    "search": {
        "url": "https://www.shuqi.com/search?keyword=${key}&page=1",
        "charset": "utf-8",
        "list": ".matchbook",
        "name": ".bname",
        "cover":"img",
        "author": ".bauthor@match->.+(?= 著)",
        "detail": "a"
    },
    "detail": {
        "status": ".lastchapter.clear>li:nth-child(3)",
        "summary":".bookDesc",
        "update": ".lastchapter.clear>li:nth-child(4)@replace->更新->",
        "words":".lastchapter.clear>li:nth-child(2)@replace->字->",
        "category":".tags",
        "lastChapter": ".lastchapter:not(.clear)>a"
    }
}
